{
  "name": "super-image-cropper",
  "version": "1.0.6",
  "main": "dist/index.js",
  "repository": "git@github.com:STDSuperman/super-image-cropper.git",
  "author": "STDSuperman <2750556766@qq.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "commit": "git add -A && git-cz",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "format:fix": "prettier --write .",
    "husky-pre-commit": "husky set .husky/_/pre-commit 'npm run test'",
    "husky-commit-msg": "husky set .husky/_/commit-msg 'npm run commitlint --edit $1'",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "npm run clean && tsc",
    "dev": "tsc --watch",
    "check": "tsc --noEmit",
    "format": "prettier --check src/**/*.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/cropperjs": "^1.3.0",
    "@types/gif.js": "^0.2.2",
    "@types/minimist": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.2",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "esbuild": "^0.14.23",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "husky": "^7.0.2",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "cropperjs": "^1.5.12",
    "gif-build-worker-js": "^1.0.2",
    "gif.js": "^0.2.0",
    "gifuct-js": "^2.1.2",
    "image-type": "^4.1.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
